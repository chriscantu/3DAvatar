{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "vercel-build": "npm run build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:avatar": "vitest --run --testNamePattern=\"Avatar\"",
    "test:avatar:visual": "vitest --run --testNamePattern=\"Avatar Visual Behavior\"",
    "test:avatar:performance": "vitest --run --testNamePattern=\"Avatar Performance Behavior\"",
    "test:avatar:behavioral": "vitest --run --testNamePattern=\"Avatar Behavioral States\"",
    "test:avatar:watch": "vitest --testNamePattern=\"Avatar\"",
    "test:avatar:full": "npm run test:avatar:visual && npm run test:avatar:performance && npm run test:avatar:behavioral"
  },
  "dependencies": {
    "@react-three/drei": "^10.4.4",
    "@react-three/fiber": "^9.2.0",
    "@types/three": "^0.178.0",
    "axios": "^1.10.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "three": "^0.178.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/browser": "^3.2.4",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "happy-dom": "^18.0.1",
    "jest-image-snapshot": "^6.5.1",
    "jsdom": "^26.1.0",
    "playwright": "^1.53.2",
    "puppeteer": "^24.12.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0",
    "vitest": "^3.2.4"
  }
}
